{
    "ConnectionStrings": {
        "DefaultConnection": "DataSource=app.db"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AppSettings": {
        "EndpointUrl": "https://<YOUR_RESOURCE>.documents.azure.com:443/",
        "AuthorizationKey": "<APIKEY>",
        "DatabaseName": "<YOUR_DATABASE>"
    }
}
